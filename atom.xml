---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>www.hikeventures.com</title>
  <link href="http://www.hikeventures.com.com/"/>
  <link type="application/atom+xml" rel="self" href="http://www.hikeventures.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.hikeventures.com/</id>

  {% for post in site.posts %}
  <entry>
    <id>http://www.batsov.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://www.hikeventures.com{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
